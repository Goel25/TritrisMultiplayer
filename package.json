{
  "name": "tritrio",
  "version": "1.0.0",
  "description": "Online multiplayer Tritris (Tetris with  Triangles)",
  "main": "index.js",
  "scripts": {
    "build": "browserify client/sketch.js -o client/bundle.js",
    "buildDebug": "browserify --debug client/sketch.js -o client/bundle.js",
    "start": "npm run build && node index.js",
    "dev": "nodemon -i client/bundle.js --exec 'npm run buildDebug && { afplay ~/Desktop/youtube/tritris/tritris/assets/move.wav & } && node index.js'"
  },
  "author": "Goel",
  "license": "ISC",
  "dependencies": {
    "browserify": "^17.0.0",
    "express": "^4.17.1",
    "p5": "^1.3.1",
    "random-seed": "^0.3.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
