{
    "name": "tritrio",
    "version": "1.0.0",
    "description": "Online multiplayer Tritris (Tetris with  Triangles)",
    "main": "index.js",
    "scripts": {
        "build": "gulp build-prod",
        "start": "gulp build && node index.js",
        "playSound": "afplay ~/Desktop/youtube/tritris/tritris/assets/move.wav &",
        "dev": "nodemon -i build --exec 'gulp build && npm run playSound && node index.js'",
        "devDebug": "nodemon -i build --exec 'gulp build && npm run playSound && node --inspect index.js'"
    },
    "author": "Goel",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.1",
        "random-seed": "^0.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-p5": "^1.3.19",
        "socket.io": "^4.1.2",
        "socket.io-client": "^4.1.2",
        "validator": "^13.6.0",

        "@babel/core": "^7.14.8",
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-react": "^7.14.5",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "gulp": "^4.0.2",
        "gulp-uglify": "^3.0.2",
        "nodemon": "^2.0.7",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0"
    },
    "type": "module",
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    }
}
